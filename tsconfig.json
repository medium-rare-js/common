{
  "compilerOptions": {
    "target": "es5",
    "strict": true,
    "sourceMap": true,
    "module": "es2015",
    "moduleResolution": "node",
    "declaration": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./src",
    "paths": {
      "@/*": ["*"]
    },
    "lib": ["es5", "es2015"],
    "allowJs": true,
    "strictPropertyInitialization": false
  }
}
